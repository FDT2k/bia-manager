{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/react-bia-manager"
        },
        {
            "name": "React-Electron",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node ${workspaceFolder}/scripts/watch-new.js",
           
            "outputCapture": "std"
          }
    ]
}